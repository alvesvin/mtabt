{
  "name": "@mtabt/utils",
  "version": "0.0.6",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./array": {
      "import": "./dist/array.js"
    },
    "./etc": {
      "import": "./dist/etc.js"
    },
    "./fs": {
      "import": "./dist/fs.js"
    },
    "./types": {
      "import": "./dist/types.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "array": [
        "./dist/array"
      ],
      "etc": [
        "./dist/etc"
      ],
      "fs": [
        "./dist/fs"
      ],
      "types": [
        "./dist/types"
      ]
    }
  },
  "files": [
    "./dist/**/*.{js,ts,map}",
    "./src",
    "./package.json"
  ],
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@mtabt/plugin": "workspace:*",
    "glob-to-regexp": "^0.4.1"
  },
  "devDependencies": {
    "@types/glob-to-regexp": "^0.4.1",
    "typescript": "^4.7.4"
  },
  "author": "Vinicius Alves",
  "license": "MIT",
  "homepage": "https://github.com/alvesvin/mtabt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvesvin/mtabt.git"
  },
  "bugs": {
    "url": "https://github.com/alvesvin/mtabt/issues"
  }
}
